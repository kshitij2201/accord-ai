
{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/hello.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/hello",
            "dest": "/api/hello.js"
        },
        {
            "src": "/api/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/favicon.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "index.js": {
            "maxDuration": 30
        },
        "api/hello.js": {
            "maxDuration": 10
        }
    }
}